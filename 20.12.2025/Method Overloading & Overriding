class MathOp {
    int add(int a, int b) {
        return a + b;
    }

    double add(double a, double b) {
        return a + b;
    }
}

class Calc extends MathOp {
    int add(int a, int b) {
        return a + b + 10;
    }

    public static void main(String[] args) {
        Calc c = new Calc();
        System.out.println(c.add(5, 5));
        System.out.println(c.add(2.5, 3.5));
    }
}
