class Solution:
    def minCut(self, s):
        n = len(s)
        dp = list(range(n))
        for i in range(n):
            for l,r in [(i,i),(i-1,i)]:
                while l>=0 and r<n and s[l]==s[r]:
                    dp[r] = min(dp[r], 0 if l==0 else dp[l-1]+1)
                    l-=1; r+=1
        return dp[-1]
