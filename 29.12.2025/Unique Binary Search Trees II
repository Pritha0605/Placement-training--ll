class Solution:
    def generateTrees(self, n: int):
        if n==0: return []
        def build(start, end):
            if start > end: return [None]
            trees = []
            for i in range(start, end+1):
                for l in build(start, i-1):
                    for r in build(i+1, end):
                        node = TreeNode(i)
                        node.left = l
                        node.right = r
                        trees.append(node)
            return trees
        return build(1, n)
