def getHint(secret, guess):
    bulls = cows = 0
    s_cnt = [0]*10
    g_cnt = [0]*10
    for s,g in zip(secret, guess):
        if s == g:
            bulls += 1
        else:
            s_cnt[int(s)] += 1
            g_cnt[int(g)] += 1
    for i in range(10):
        cows += min(s_cnt[i], g_cnt[i])
    return f"{bulls}A{cows}B"
