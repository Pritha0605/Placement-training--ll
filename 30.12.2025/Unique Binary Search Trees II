class Solution:
    def generateTrees(self, n):
        def gen(l, r):
            if l > r: return [None]
            res = []
            for i in range(l, r+1):
                for left in gen(l, i-1):
                    for right in gen(i+1, r):
                        root = TreeNode(i, left, right)
                        res.append(root)
            return res
        return gen(1, n) if n else []
