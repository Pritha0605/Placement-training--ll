def findStrobogrammatic(n):
    def helper(m, n):
        if m == 0: return [""]
        if m == 1: return ["0","1","8"]
        res = []
        for mid in helper(m-2, n):
            for a,b in [("0","0"),("1","1"),("6","9"),("8","8"),("9","6")]:
                if m != n or a != "0":
                    res.append(a + mid + b)
        return res
    return helper(n, n)
