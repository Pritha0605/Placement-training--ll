class Vector2D:
    def __init__(self, vec):
        self.vec = []
        for row in vec:
            for x in row:
                self.vec.append(x)
        self.i = 0

    def next(self):
        val = self.vec[self.i]
        self.i += 1
        return val

    def hasNext(self):
        return self.i < len(self.vec)
