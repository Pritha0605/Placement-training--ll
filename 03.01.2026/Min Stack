class MinStack:
    def __init__(self):
        self.s = []
        self.min = []

    def push(self, val):
        self.s.append(val)
        self.min.append(val if not self.min else min(val, self.min[-1]))

    def pop(self):
        self.s.pop()
        self.min.pop()

    def top(self):
        return self.s[-1]

    def getMin(self):
        return self.min[-1]
