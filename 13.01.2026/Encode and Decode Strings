class Codec:
    def encode(self, strs):
        return ''.join(f"{len(s)}#{s}" for s in strs)

    def decode(self, s):
        res, i = [], 0
        while i < len(s):
            j = s.find('#', i)
            l = int(s[i:j])
            res.append(s[j+1:j+1+l])
            i = j+1+l
        return res
